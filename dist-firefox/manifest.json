{
  "manifest_version": 3,
  "name": "Browse Bot (Firefox)",
  "version": "0.0.0",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "notifications",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "assets/service_worker.ts-I87hgpH8.js"
    ]
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "src/content.ts"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/content.ts-Dapdx9IB.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-Dapdx9IB.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "sidebar_action": {
    "default_panel": "index.html",
    "default_title": "Browse Bot"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "web-walker@1752650324951.org"
    }
  },
  "action": {
    "default_title": "Открыть Browse Bot"
  },
  "omnibox": {
    "keyword": "@ai"
  }
}
